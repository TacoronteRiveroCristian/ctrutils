# Configuracion de coverage
[run]
source = ctrutils
branch = True
omit =
    */tests/*
    */test_*.py
    */__init__.py
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

precision = 2
show_missing = True
skip_covered = False
fail_under = 0

[html]
directory = tests/coverage
